{
  "version": 2,
  "buildCommand": "python build.py",
  "installCommand": "uv pip install --system --only-binary :all: -r requirements.txt",
  "outputDirectory": "staticfiles",
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ]
}


