{
  "version": 2,
  "buildCommand": "uv python build.py",
  "installCommand": "uv pip install --system -r requirements.txt --target .vercel_python_packages",
  "outputDirectory": "staticfiles",
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
